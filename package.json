{
	"name": "eleventy-base-blog",
	"version": "8.0.0",
	"description": "A starter repository for a blog web site using the Eleventy site generator with Sveltia CMS.",
	"scripts": {
		"build": "npx @11ty/eleventy",
		"build-ghpages": "npx @11ty/eleventy --pathprefix=/sveltia-cms-eleventy-base-blog/",
		"start": "npx @11ty/eleventy --serve --quiet",
		"debug": "DEBUG=Eleventy* npx @11ty/eleventy",
		"debugstart": "DEBUG=Eleventy* npx @11ty/eleventy --serve --quiet",
		"benchmark": "DEBUG=Eleventy:Benchmark* npx @11ty/eleventy"
	},
	"keywords": [
		"sveltia-cms",
		"eleventy"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/devurandom/sveltia-cms-eleventy-base-blog.git"
	},
	"contributors": [
		{
			"name": "Dennis Schridde",
			"email": "heri@mailbox.org"
		},
		{
			"name": "Zach Leatherman",
			"email": "zachleatherman@gmail.com",
			"url": "https://zachleat.com/"
		},
		{
			"name": "Dan Urbanowicz",
			"email": "dan.urbanowicz@gmail.com",
			"url": "https://danurbanowicz.com"
		}
	],
	"license": "MIT",
	"engines": {
		"node": ">=14"
	},
	"bugs": {
		"url": "https://github.com/devurandom/sveltia-cms-eleventy-base-blog/issues"
	},
	"homepage": "https://github.com/devurandom/sveltia-cms-eleventy-base-blog",
	"devDependencies": {
		"@11ty/eleventy": "^2.0.1",
		"@11ty/eleventy-img": "^3.1.1",
		"@11ty/eleventy-navigation": "^0.3.5",
		"@11ty/eleventy-plugin-bundle": "^1.0.4",
		"@11ty/eleventy-plugin-rss": "^1.2.0",
		"@11ty/eleventy-plugin-syntaxhighlight": "^5.0.0",
		"js-yaml": "^4.1.0",
		"luxon": "^3.3.0",
		"markdown-it-anchor": "^8.6.7"
	},
	"trustedDependencies": ["sharp"],
	"overrides": {
		"sharp": "0.33.0-alpha.11"
	}
}
